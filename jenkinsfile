    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building the App'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing the App'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying the App'
            }
        }
stage('change') {
            steps {
                echo 'change the App'
            }
        }
    }
}
